#define _GNU_SOURCE
#include <limits.h>
#include <signal.h>
#include <stdbool.h>
#include <stdio.h>
#include <stdlib.h>
#include <string.h>
#include <unistd.h>

void phrase(char *input, char **args) {
  int i = 0;
  char *token = NULL;
  char *ptr = NULL;
  while ((token = strtok_r(input, " \t\n\r", &ptr))) {
    args[i] = token;
    i++;
    input = NULL;
  }
  i++;
  args[i] = NULL;
}
void printArgs(char **args) {
  for (int i = 0; args[i] != NULL; i++) {
    write(STDOUT_FILENO, args[i], strlen(args[i]));
    write(STDOUT_FILENO, "\n", 1);
  }
}
int main() {
  char cwd_buffer[1024];
  char input[1024];
  char *args[64];
  while (true) {
    getcwd(cwd_buffer, sizeof(cwd_buffer));
    size_t length = strlen(cwd_buffer);
    write(STDOUT_FILENO, cwd_buffer, length);
    write(STDOUT_FILENO, " ", 1);
    ssize_t inputLenght = read(STDIN_FILENO, input, sizeof(input) - 1);
    input[inputLenght] = '\0';
    write(STDOUT_FILENO, input, strlen(input));
    phrase(input, args);
    printArgs(args);
    free(input);
  }
  // write(STDOUT_FILENO, "\n", 1);
}
